<Window x:Class="Crawl_Data_Tiki.CrawlData.Program"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Crawl_Data_Tiki.CrawlData"
        mc:Ignorable="d"
        Title="Program" Height="130" Width="250"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <ProgressBar x:Name="progressBar" Width="200" Height="20" Minimum="0" Maximum="100" />
        <!-- Button để mô phỏng việc tăng giá trị tiến trình -->
        <Button Width="200" x:Name="NameProcess" VerticalAlignment="Top" Margin="10"/>
        <Button Width="200" x:Name="ExitBtn" Content="Exit" Click="ExitBtn_Click" VerticalAlignment="Bottom" Margin="10"/>

    </Grid>
</Window>
